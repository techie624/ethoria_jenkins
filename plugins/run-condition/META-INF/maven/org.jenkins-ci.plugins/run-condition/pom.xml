  <modelVersion>4.0.0</modelVersion>
  <groupId>org.jenkins-ci.plugins</groupId>
  <artifactId>run-condition</artifactId>
  <version>1.7</version>
  <packaging>hpi</packaging>
  <name>Run Condition Plugin</name>
